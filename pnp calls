          url: "https://pfizer.sharepoint.com/sites/ARMS-Dev/_api/web/lists/getbytitle('" + listTitle + "')/items?$select=Week,User/Id&$filter=UserId eq "+_spPageContextInfo.userId+"&$expand=User&$top=5000",
