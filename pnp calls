function getLastWeeksEffort(){
var urlList = "";
    var filter = ""; 
     urlList =  _spPageContextInfo.webAbsoluteUrl + "/_api/web/lists/getbytitle('EffortTrackingList1')/items?$select=Week&$filter=UserId eq "+_spPageContextInfo.userId+" and Status eq 'Submitted'&$orderby=Week desc&$top=100"+"";
		console.log(urlList); 
		 var resultArray = [];
            			//urlList = _spPageContextInfo.webAbsoluteUrl + "/_api/web/lists/getbytitle('RemediationTracker')/items";
    $.ajax({
        url: urlList,
        type: "GET",
        async: false,
        headers: { "Accept": "application/json",
               "Access-Control-Allow-Origin":"*" },
        success: function (data) {
        console.log("1235",data);
         var dateval=[];

        if(data.value.length >0){
        //showPopupValues(data.value);
        var dateval=[];
        for(var i=0;i<data.value.length;i++){
         var today=data.value[i].Week;

        today = moment(today).format("YYYY-MM-DD");
        dateval.push(today);

        }
        dateval=[... new Set(dateval)];
        dateval=dateval.slice(0,4);
        console.log(dateval);
         showPopupValues(dateval);

        
        }
        else{
         showPopupValues(dateval);

        }
                           },
        error: function (data) {
        //alert('error');
      //deferred.resolve(null);
        }
    });
}

function showPopupValues(param){
$('#PrevBox').empty();
var html="";
if(param.length ==0){
html+='<div><span><b>No recent data found</b></span></div>';
}
else{
 for(var i=0;i<param.length;i++){
 var today=param[i];
 //add(1,'days');
 var prevDay= moment(today).subtract(4, "days");
 prevDay = moment(prevDay).format("MM/DD/YYYY");
today = moment(today).format("MM/DD/YYYY");

 
 html+='<div style="margin-bottom:5px;"><span><b>'+prevDay+" -  " +today +'   </b></span>'+
 		'<button type="button" class="buttonRowAdd popButtons" id="'+param[i]+'" onclick="getEffortDetailsWithId(this.id)">Select</button>'+
   '</div>';
 		
 }
}
$('#PrevBox').append(html);
}
function showEffort(param){
console.log("show efforts:" + param);
$(".se-pre-con").css("display", "block");

  $("#tblDashboard").DataTable().destroy();
  
var efforts=getEffortDetails(param);
console.log("Efforts" + efforts);

 var finalAllHTML ='';
 var filterManager ='';
 var filterAll='';



           if (efforts!= null && efforts.length > 0) {
            for (var i = 0; i < efforts.length; i++) {
            
            
            
             			                                        
            						
				var category="";
				if(efforts[i].Categories.Title !=null)
					category=efforts[i].Categories.Title;	
				var subcategory="";
				if(efforts[i].SubCategories.Title !=null)
					subcategory=efforts[i].SubCategories.Title;
				var subSubcategory="";
				if(efforts[i].SubNCategory.Title !=null)
				    subSubcategory=efforts[i].SubNCategory.Title;
				
				

               
			 var HTML='<tr><td>'+  +'</td>';
	
				       HTML+='<td data-toggle="tooltip" title=' + category + '>' + category + '</td>';
                HTML+='<td title="tst">' + subcategory  + '</td>';
                HTML+='<td>' + subSubcategory + '</td>';
                HTML+='<td>'+Mon-Fri+'</td></tr>';
                                                              
               // HTML+='<td><a target="_self" href="' + linkToRemedy + '">' + remID + '</a></td>'
                	console.log(HTML);			
				//HTML+='<td>' + finalDueDate + '</td></tr>';
                  finalAllHTML += HTML;
                $("#tblDashboardBody").html(finalAllHTML);
                //allCount += 1;
              
                             
            
            }
           }
                     // $("#tblManageRequestsBody").html(html);
          
	      
           
            $("#tblDashBoard").DataTable().clear();
            $("#tblDashBoard").DataTable().destroy();
                       
	        createDatatable(); 
	      
	   //   $(".dispBuffer").css("display", "none");
	      setTimeout(()=> {$(".se-pre-con").css("display", "none")},600 );
	      

}

function createDatatable() {
   // var isadmin=validateUser("PRT Site Admin-Dev");
   console.log("create table");
  
   
    var colDefs = [
            { "width": "10%", "targets": 0, "className": 'dt-body-left' },
            { "width": "10%", "targets": 1, "className": 'dt-body-left' },
            { "width": "10%", "targets": 2, "className": 'dt-body-left' },
            { "width": "5%", "targets": 3, "className": 'dt-body-left' },
            { "width": "5%", "targets": 4, "className": 'dt-body-left' },
           // { "width": "5%", "targets": 5, "className": 'dt-body-left' },
           // { "width": "5%", "targets": 6, "className": 'dt-body-left' },
          //  { "width": "5%", "targets": 7, "className": 'dt-body-left' },
          //  { "width": "5%", "targets": 8, "className": 'dt-body-left' },
           // { "width": "10%", "targets": 9, "className": 'dt-body-left' },
           // { "width": "5%", "targets": 10, "className": 'dt-body-left' },

];
    $("#tblDashBoard").DataTable().clear();
    var table = $("#tblDashboard").DataTable({        
        scrollX: false,
        scrollCollapse: true,
        autoWidth: true,
