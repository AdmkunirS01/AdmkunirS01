   }

  function isNumber(evt) {
    evt = (evt) ? evt : window.event;
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if(charCode == 46 ){
    return true;
    }
   else  if (charCode > 31 && (charCode < 48 || charCode > 57)) {
        return false;
    }
    
    return true;
}
  function showChangeDate(param){
  console.log("showchange date");
  $('#showWeekDate').empty();
	var today=param;
  console.log(moment(today).format('d'));
  var noDays=moment(today).format('d');
   var prevDay= today;
   var nextDay=today;
   noDays=parseInt(noDays,10);
  if(noDays<=5 && noDays!=0){
  var n=5-noDays;
  nextDay= moment(today).add(n+2, "days");
 nextDay= moment(nextDay).format("MM/DD/YY");
 prevDay= moment(today).subtract((5-n-1), "days");
	prevDay = moment(prevDay).format("MM/DD/YY");


  }
  else{
   if(noDays==0){
   prevDay= moment(today).subtract(6, "days");
	prevDay = moment(prevDay).format("MM/DD/YY");
	nextDay= moment(today).subtract(0, "days");
 	nextDay= moment(nextDay).format("MM/DD/YY");

   }
   else{
	prevDay= moment(today).subtract(5, "days");
	prevDay = moment(prevDay).format("MM/DD/YY");
	nextDay= moment(today).subtract(-1, "days");
 	nextDay= moment(nextDay).format("MM/DD/YY");
 	}

  
  }
  $('#showWeekDate').append(""+prevDay+" -  "+nextDay+"");
 //add(1,'days');
 //var prevDay= moment(today).subtract(4, "days");
 //prevDay = moment(prevDay).format("MM/DD/YYYY");
//today = moment(today).format("MM/DD/YYYY");

  }
