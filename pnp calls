    function setDefaultUserName() {
    var txtName = document.getElementById('txtName');

    title = $().SPServices.SPGetCurrentUser({
        fieldName: "Title"
    });


    if (txtName != null && txtName != undefined && txtName != '') {
        txtName.value = title;
    }
    //userId = $().SPServices.SPGetCurrentUser({
    //    fieldName: "ID"
   // });

}
function getManagerDetails(){
$.ajax({  
        url:  _spPageContextInfo.webAbsoluteUrl + "/_api/web/lists/getbytitle('ManagerAssigneeMapping')/items?$select=Manager/Title,Manager/Id,Id&$filter=AssigneeId eq "+_spPageContextInfo.userId+"&$expand=Manager"+"",  
        headers: {  
            "Access-Control-Allow-Origin":"*",
            "Accept": "application/json"  
        },  
        async: false,  
        success: function (data) {  
            console.log(data);
            if(data.value.length >0){
             var manager=data.value;
             ManagerId=manager[0].Manager.Id;
            console.log(ManagerId);
            }
            $("#ShowNote").remove();

			if(ManagerId == 0){
			
			$("#warningHeader").append("<span id='ShowNote'>Note: Manager for the logged in user is not assigned</span>");

			
			}
			else{
			//$("#warningHeader").append("<span>Note: Manager for the logges in user is not assigned</span>");
			
			$("#ShowNote").remove();
			}
                       
		},  
        error: function (data) {  
            alert("An error occurred. Please try again."); 
              $(".se-pre-con").css("display", "none");
              CreateErrorLog("ManagerAssigneeMapping",data); 
        }  
    });  


}
