  function getFixedCategory(){
   $.ajax({  
        url:  _spPageContextInfo.webAbsoluteUrl + "/_api/web/lists/getbytitle('Categories')/items?$select=Title,DefaultValue,Id&$filter=Active eq 1 and SortOrder eq 1&$orderBy=SortOrder"+"",  
        headers: {  
            "Access-Control-Allow-Origin":"*",
            "Accept": "application/json"  
        },  
        async: false,  
        success: function (data) {  
            console.log(data);
            if(data.value.length>0){
             fixedCategoryId=data.value[0].Id;
              fixedCategoryDefaultValue=data.value[0].DefaultValue;

            console.log(fixedCategoryId);
            
            }

                        
		},  
        eror: function (data) {  
            alert("An error occurred. Please try again."); 
            CreateErrorLog("Categories",data); 
 
        }  
    });  

  
  }
   function ShowTotalHours(){
