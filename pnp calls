function AddRowWithValues(Values){
 
 for(var i=0;i<Values.length;i++){
 var id = $('.tableRow').last().attr('id');
   id =(id)? id.replace(/tableRow/, ''):"0";
    console.log(id);
    id=parseInt(id,10)+1;
    valuesId.push(id);
    console.log(valuesId);

 var markup = 
            '<tr class="row tableRow" id="tableRow'+id+'" style="text-align: center;">'+
           '<td ><input id="timesheet'+id+'" type="checkbox" name="timesheet" /></td>'+
            '<td id="catgs'+id+'"><select class="drpdwnClass" data-toggle="tooltip"  title='+Values[i].Categories.Title.split(" ")+' id="drpCat'+id+'" onchange="AppendSubcategoriesDropdown(drpCat'+id+')" value='+Values[i].Categories.Id+' style="width: 95%;">'+
              '<option>--select--</option></select></td><td>'+
            '<select class="drpdwnClass" id="drpSubCat'+id+'" onchange="AppendSubNcategoriesDropdown(drpSubCat'+id+')"  style="width: 95%;">'+
             '<option>--select--</option></select><span class="starcat'+id+'" style="color: red; font-size: 12">*</span> </td>'+
                  '<td style="display:none">'+
            '<select class="drpdwnClass" id="drpSubNCat'+id+'" style="width: 95%;" >'+
            ' <option>--select--</option></select> </td> <td>'+
            '<input id="hours'+id+'" type="text" value='+Values[i].Effort+' style="width: 130px;" onkeypress="return isNumber(event)" onchange="ShowTotalHours()"/> </td>'
            +'<td style="display:none;"><input type="text" id="Item'+id+'" value="'+Values[i].Id+'"></span></td>'+

            '</tr>';

            $("#effortBody").append(markup);
            $(".starcat"+id).css('display', 'none');
            AppendCatgeoriesDropdownWithValues("drpCat"+id,Values[i]);
            AppendSubcategoriesDropdownWithValues("drpSubCat"+id,Values[i]);
            AppendSubNcategoriesDropdownWithValues("drpSubNCat"+id,Values[i]);
            ShowTotalHours();

}
            
 }

function AddRowWithValuesId(Values){
 
 for(var i=0;i<Values.length;i++){
 var id = $('.tableRow').last().attr('id');
   id =(id)? id.replace(/tableRow/, ''):"0";
    console.log(id);
    id=parseInt(id,10)+1;
    valuesId.push(id);
    console.log(valuesId);

 var markup = 
            '<tr class="row tableRow" id="tableRow'+id+'" style="text-align: center;">'+
           '<td><input id="timesheet'+id+'" type="checkbox" name="timesheet" /></td>'+
            '<td id="catgs'+id+'"><select class="drpdwnClass" data-toggle="tooltip" title='+Values[i].Categories.Title.split(" ")+' id="drpCat'+id+'" onchange="AppendSubcategoriesDropdown(drpCat'+id+')" value='+Values[i].Categories.Id+' style="width: 95%;">'+
              '<option>--select--</option></select></td><td>'+
            '<select class="drpdwnClass" id="drpSubCat'+id+'" onchange="AppendSubNcategoriesDropdown(drpSubCat'+id+')"  style="width: 95%;">'+
             '<option>--select--</option></select><span class="starcat'+id+'" style="color: red; font-size: 12">*</span> </td>'+
                  '<td style="display:none">'+
            '<select class="drpdwnClass" id="drpSubNCat'+id+'" style="width: 95%;">'+
            ' <option>--select--</option></select> </td> <td>'+
            '<input id="hours'+id+'" type="text" value='+Values[i].Effort+' style="width: 130px;" onkeypress="return isNumber(event)" onchange="ShowTotalHours()"/> </td>'
            +'<td style="display:none;"><input type="text" id="Item'+id+'" value="0"></span></td>'+
            '</tr>';

            $("#effortBody").append(markup);
            $(".starcat"+id).css('display', 'none');
            AppendCatgeoriesDropdownWithValues("drpCat"+id,Values[i]);
            AppendSubcategoriesDropdownWithValues("drpSubCat"+id,Values[i]);
            AppendSubNcategoriesDropdownWithValues("drpSubNCat"+id,Values[i]);
            closeForm();
            ShowTotalHours();

}
            
 }

