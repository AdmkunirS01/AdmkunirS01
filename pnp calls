function createDatatable() {
   // var isadmin=validateUser("PRT Site Admin-Dev");
   console.log("create table");
  
   
    var colDefs = [
            { "width": "10%", "targets": 0, "className": 'dt-body-left' },
            { "width": "10%", "targets": 1, "className": 'dt-body-left' },
            { "width": "10%", "targets": 2, "className": 'dt-body-left' },
            { "width": "5%", "targets": 3, "className": 'dt-body-left' },
            { "width": "5%", "targets": 4, "className": 'dt-body-left' },
           // { "width": "5%", "targets": 5, "className": 'dt-body-left' },
           // { "width": "5%", "targets": 6, "className": 'dt-body-left' },
          //  { "width": "5%", "targets": 7, "className": 'dt-body-left' },
          //  { "width": "5%", "targets": 8, "className": 'dt-body-left' },
           // { "width": "10%", "targets": 9, "className": 'dt-body-left' },
           // { "width": "5%", "targets": 10, "className": 'dt-body-left' },

];
    $("#tblDashBoard").DataTable().clear();
    var table = $("#tblDashboard").DataTable({        
        scrollX: false,
        scrollCollapse: true,
        autoWidth: true,
        //paging: true,
        columnDefs: colDefs,
        bSortCellsTop: true,
        bSort: true,
         orderCellsTop: true,
   		 responsive: true,
   		  language: {
      "emptyTable": "No records to display"
    },
        dom: '<B>lftip',
        buttons: [
				 {
                           }],
            
	});
	$('.dt-buttons')
    .appendTo( $('.exceldownload'));
    var btn = $(".dt-button");
    btn.removeClass();
    btn.addClass('btn btn-outline-primary smallbtn');
	//var isadmin=validateUser("PRT Site Admin-Dev");
	
	$("#tblDashboard thead .filterh").each(function (i) {	
		var colIndex=i;
	 	 var isfilter = (i == 0 ? true : false);
	  var select = $('<select style="width:90%"><option value="clear">' + '</option></select>')
                .appendTo($(this).empty())
                .on('change', function () {
                    var val = $(this).val();
					if(val == 'clear')
					{
						table.column(colIndex)
                        .search('', true, false)
                        .draw();

					}
                    else
                    {
							var escape_parenthesis = new RegExp( '(\\' + [ '(', ')'].join('|\\') + ')', 'g' );        
                            var searchtext = $(this).val().replace( escape_parenthesis, '\\$1' );   

                            table.column(colIndex)
                                .search('^' + searchtext + '$', true, false)
                                .draw(); 
                                   }
                });
                
                if(isfilter){
                    var remItems = [];
                    table.column(colIndex).nodes().to$().each(function (d, j) {
                        var thisRem = $(j)[0].innerText;
                        if ($.inArray(thisRem, remItems) === -1) remItems.push(thisRem);
                    });

                    remItems.sort();

                    $.each(remItems, function (i, item) {
                        select.append('<option value="' + item + '">' + item + '</option>');
                    });
                }
                else{
                table.column(colIndex).data().unique().sort().each(function (d, j) {
                    if (d === '') {
                        d = '(Empty)';
                    }
                    select.append('<option value="' + (d=='(Empty)'?'':d) + '">' + d + '</option>');
                });  
                }


				});

 }
  


