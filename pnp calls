    .appendTo( $('.exceldownload'));
    var btn = $(".dt-button");
    btn.removeClass();
    btn.addClass('btn btn-outline-primary smallbtn');
	//var isadmin=validateUser("PRT Site Admin-Dev");
	
	$("#tblDashboard thead .filterh").each(function (i) {	
		var colIndex=i;
	 	 var isfilter = (i == 0 ? true : false);
	  var select = $('<select style="width:90%"><option value="clear">' + '</option></select>')
                .appendTo($(this).empty())
                .on('change', function () {
                    var val = $(this).val();
					if(val == 'clear')
					{
						table.column(colIndex)
                        .search('', true, false)
                        .draw();

					}
                    else
                    {
							var escape_parenthesis = new RegExp( '(\\' + [ '(', ')'].join('|\\') + ')', 'g' );        
                            var searchtext = $(this).val().replace( escape_parenthesis, '\\$1' );   

                            table.column(colIndex)
                                .search('^' + searchtext + '$', true, false)
                                .draw(); 
                                   }
                });
                
                if(isfilter){
                    var remItems = [];
                    table.column(colIndex).nodes().to$().each(function (d, j) {
                        var thisRem = $(j)[0].innerText;
                        if ($.inArray(thisRem, remItems) === -1) remItems.push(thisRem);
                    });

                    remItems.sort();

                    $.each(remItems, function (i, item) {
                        select.append('<option value="' + item + '">' + item + '</option>');
                    });
                }
                else{
                table.column(colIndex).data().unique().sort().each(function (d, j) {
                    if (d === '') {
                        d = '(Empty)';
                    }
                    select.append('<option value="' + (d=='(Empty)'?'':d) + '">' + d + '</option>');
                });  
                }


				});

 }
  

==========================function AppendSubcategoriesDropdownWithValues(drpCatId, assignedValue) {
    var categor = drpCatId;
    console.log(categor);
    var id = categor;
    id = id.replace(/drpSubCat/, '');
    console.log(id);
    id = parseInt(id, 10);

    $('#drpSubCat' + id).empty();
    var cat = "<option>--select--</option>";

    // Arrays for matched and unmatched IDs
    var matchedIds = [];
    var unmatchedIds = [];

    for (var i = 0; i < SubCategories.length; i++) {
        var valuedrp = assignedValue.Categories.Id;
        if (valuedrp == SubCategories[i].Category.Id) {
            cat += '<option value=' + SubCategories[i].Id + '>' + SubCategories[i].Title + '</option>';

            // Compare SubCategories[i].Id and assignedValue.SubCategories.Id
            if (assignedValue.SubCategories != null && SubCategories[i].Id == assignedValue.SubCategories.Id) {
                matchedIds.push(SubCategories[i].Id);
            }
        }
        else {
            unmatchedIds.push(SubCategories[i].Id);
        }
    }

    $('#drpSubCat' + id).append(cat);

    //fixed category Id
    if (assignedValue.SubCategories != null) {
        let element = document.getElementById(drpCatId);
        element.value = assignedValue.SubCategories.Id;
    }

    console.log("Matched IDs:", matchedIds);
    console.log("Unmatched IDs:", unmatchedIds);
}

====
function AppendSubcategoriesDropdownWithValues(drpCatId, assignedValue) {
    var categor = drpCatId;
    console.log(categor);
    var id = categor;
    id = id.replace(/drpSubCat/, '');
    console.log(id);
    id = parseInt(id, 10);

    $('#drpSubCat' + id).empty();
    var cat = "<option>--select--</option>";

    for (var i = 0; i < SubCategories.length; i++) {
        var valuedrp = assignedValue.Categories.Id;
        if (valuedrp == SubCategories[i].Category.Id) {
            cat += '<option value=' + SubCategories[i].Id + '>' + SubCategories[i].Title + '</option>';
        }
    }

    $('#drpSubCat' + id).append(cat);

    // Fixed category Id
    if (assignedValue.SubCategories != null) {
        let element = document.getElementById(drpCatId);
        element.value = assignedValue.SubCategories.Id;
    }

    // Compare SubCategories[i].Id and assignedValue.SubCategories.Id
    if (assignedValue.SubCategories != null) {
        var subCategoryId = assignedValue.SubCategories.Id;
        for (var i = 0; i < SubCategories.length; i++) {
            if (SubCategories[i].Id === subCategoryId) {
                // Match found, do something with SubCategories[i]
                console.log("Match found. Subcategory Title:", SubCategories[i].Title);
                console.log("Match found. Subcategory ID:", SubCategories[i].Id);
            }
        }
    } else {
        // No match found
        console.log("No match found for Subcategory ID:", subCategoryId);
    }
}

