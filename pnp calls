    console.log("first",firstDayofWeek)
    
    const lastDayofWeek= new Date(today.getFullYear(),today.getMonth(),today.getDate()+(6-today.getDay()+1)); 
    console.log("last",lastDayofWeek)
    if(today>=firstDayofWeek&&today<=lastDayofWeek)
    {
    console.log("Today date current week");
