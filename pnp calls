function AppendSubcategoriesDropdownWithValues(drpCatId,assignedValue){
var categor=drpCatId;
console.log(categor);
var id = categor;
   id = id.replace(/drpSubCat/, '');
    console.log(id);
    id=parseInt(id,10);
  

$('#drpSubCat'+id).empty();
var cat="<option>--select--</option>";
for(var i=0;i<SubCategories.length;i++){
var valuedrp=assignedValue.Categories.Id;
	if(valuedrp ==SubCategories[i].Category.Id){
	cat+='<option value='+SubCategories[i].Id+'>'+SubCategories[i].Title+'</option>';
	}
}
 $('#drpSubCat'+id).append(cat);
 //fixed category Id
 if(assignedValue.SubCategories!= null){

 let element = document.getElementById(drpCatId);
    element.value = assignedValue.SubCategories.Id;
    }


}

function GetSubCategories(){
		 
    $.ajax({  
        url:  _spPageContextInfo.webAbsoluteUrl + "/_api/web/lists/getbytitle('Subcategories')/items?$select=Title,Id,MandetorySubcategory,Category/Title,Category/Id&$filter=Active eq 1&$orderBy=SortOrder&$expand=Category"+"",  
        headers: {  
            "Access-Control-Allow-Origin":"*",
            "Accept": "application/json"  
        },  
        async: false,  
        success: function (data) {  
            console.log(data);
           SubCategories=data.value;
            console.log(SubCategories,"==>");

                        
		},  
        eror: function (data) {  
            alert("An error occurred. Please try again.");  
            CreateErrorLog('Subcategories',data);
        }  
    });  
	
}
