function GetItemTypeForListName(name) {
    return "SP.Data." + name.charAt(0).toUpperCase() + name.split(" ").join("").slice(1) + "ListItem";
}

function closePopUp() {	
    $("#ticketDetail").css("display", "none");
}
// and Week eq datetime'"+param+"'
 function getEffortDetails(param) {
 $('#effortBody').empty();
  $(".se-pre-con").css("display", "block");  

 valuesId=[];
 console.log(param);
 var today=param;
 var noDays=moment(today).format('d');
  
   var WeeknendDay=today;
   noDays=parseInt(noDays,10);
  if(noDays<=6 && noDays !=0){
  var n=6-noDays;
  WeeknendDay= moment(today).add(n+2, "days");
 
 

  }
  else{
  	WeeknendDay= moment(today).add(0, "days");
 	

  
  }
  
  var date1=new Date();
  date1=moment(date1).format("YYYY-MM-DD")
//console.log(date1);
var noDaysToday=moment(date1).format('d');
  
   var WeeknendToDay=date1;
   noDaysToday=parseInt(noDaysToday,10);
  if(noDaysToday<=6 && noDaysToday!=0){
  var n=6-noDaysToday;
  WeeknendToDay= moment(date1).add(n+2, "days");
 
 

  }
  else{
  	WeeknendToDay= moment(date1).add(0, "days");
 	

  
  }
  var finalCheckDate=moment(WeeknendToDay).format("YYYY-MM-DD")
finalCheckDate= finalCheckDate+'T00:00:00.000Z';

  


 var nextday  = moment(WeeknendDay).add(1,'days');

var curtoday = moment(WeeknendDay).format("YYYY-MM-DD");
var currentDate = curtoday +'T00:00:00.000Z';

nextday = moment(nextday).format("YYYY-MM-DD");
var nextDate = nextday+'T00:00:00.000Z';
 console.log(currentDate );
 console.log(nextDate);
 var finalArray = [];
    var urlList = "";
    var filter = ""; 
     urlList =  _spPageContextInfo.webAbsoluteUrl + "/_api/web/lists/getbytitle('EffortTrackingList1')/items?$select=*,User/Title,User/ID,Categories/Title,Categories/Id,SubCategories/Title,SubCategories/Id,SubNCategory/Id,Manager/Id&$filter=UserId eq "+_spPageContextInfo.userId+" and Week ge datetime'" +currentDate+ "' and Week le datetime'" +nextDate+"'&$expand=Categories,SubCategories,SubNCategory,Manager,User"+"";
		console.log(urlList); 
		 var resultArray = [];
            			
    $.ajax({
        url: urlList,
        type: "GET",
        async: false,
        headers: { "Accept": "application/json",
               "Access-Control-Allow-Origin":"*" },
        success: function (data) {
        

        console.log("1368",data);
        if(data.value.length>0){
        
 
 
 

 
        AddRowWithValues(data.value)
        if(data.value[0].Status == "Submitted"){
        disableButtons();
        ShowTotalHours();
         
        showRecallButton();
        console.log("81items",data.value[0].Week)
          
        var today= new Date(data.value[0].Week);
         var today1= new Date(today.getFullYear(),today.getMonth(),today.getDate()-today.getDay())
         console.log("1225",today1)
         const firstDayofWeek= new Date(today.getFullYear(),today.getMonth(),today.getDate()-today.getDay()-6);
console.log("first",firstDayofWeek)

const lastDayofWeek= new Date(today.getFullYear(),today.getMonth(),today.getDate()+(6-today.getDay()-6)); 
console.log("last",lastDayofWeek)
if(today1>=firstDayofWeek&&today1<=lastDayofWeek)
{
console.log("Today date current week");
document.getElementById("notify").style.display = "none";

} 
       
        }
        else{
        hideRecallButton();
        EnableButtons();
         ShowTotalHours();
        }
        }
        else{
        //For Fixed Category
        //AddRow();
        AddRowFixedCategory();
        EnableButtons();
         ShowTotalHours();
          hideRecallButton();
          console.log("this date not in current week");
  document.getElementById("notify").style.display = "block";
        }
        //  $(".se-pre-con").css("display", "none");
        setTimeout(function(){

           $(".se-pre-con").css("display", "none");  
   		},4000);
  
                   },
        error: function (data) {
        //alert('error');
      //deferred.resolve(null);
      	CreateErrorLog("EffortTrackingList1",data); 

        $(".se-pre-con").css("display", "none");  
        }
    });
    console.log("FInalArray" + finalArray);
	return finalArray;
       
}
function getEffortDetailsWithId(param1) {
var param=param1;
console.log(param);

for(var i=0;i<valuesId.length;i++){
var id=valuesId[i];
if(parseInt($('#Item'+id).val(),10)>0){
deleteValuesFromList(parseInt($('#Item'+id).val(),10));
}

}
 
 valuesId=[];
 console.log(param);
 var today=param;
 var nextday  = moment(today).add(1,'days');

today = moment(today).format("YYYY-MM-DD");
var currentDate = today+'T00:00:00.000Z';

nextday = moment(nextday).format("YYYY-MM-DD");
var nextDate = nextday+'T00:00:00.000Z';
 console.log(currentDate );
 console.log(nextDate);
 id=0;
 var finalArray = [];
    var urlList = "";
    var filter = ""; 
    $('#effortBody').empty();
     urlList =  _spPageContextInfo.webAbsoluteUrl + "/_api/web/lists/getbytitle('EffortTrackingList1')/items?$select=*,User/Title,User/ID,Categories/Title,Categories/Id,SubCategories/Title,SubCategories/Id,SubNCategory/Id,Manager/Id&$filter=UserId eq "+_spPageContextInfo.userId+" and Week ge datetime'" +currentDate+ "' and Week le datetime'" +nextDate+"' and Status eq 'Submitted'&$expand=Categories,SubCategories,SubNCategory,Manager,User"+"";
		console.log(urlList); 
		 var resultArray = [];
            			//urlList = _spPageContextInfo.webAbsoluteUrl + "/_api/web/lists/getbytitle('RemediationTracker')/items";
    $.ajax({
        url: urlList,
        type: "GET",
        async: false,
        headers: { "Accept": "application/json",
               "Access-Control-Allow-Origin":"*" },
        success: function (data) {
        console.log(data);
        if(data.value.length>0){
        AddRowWithValuesId(data.value)
        for(let i=0;i<data.value.length;i++){
       //AddRowWithValuesId(data.value[i]);
       id++;
         let getObj=Categories.filter((res)=>res.Title==data.value[i]?.Categories.Title);
  console.log(getObj,data.value[i]?.Categories.Title,'testing');
  if((getObj[0].MandatorySubcategory==true)){
