function AppendCatgeoriesDropdownWithFixedCategoryValues(drpCatid,assignedValue){
var cat="";
for(var i=0;i<Categories.length;i++){
	cat+='<option value='+Categories[i].Id+' >'+Categories[i].Title+'</option>';
}
 $('#'+drpCatid).append(cat);
  //$('#'+drpCatid).val(assignedValue);
   let element = document.getElementById(drpCatid);
    element.value = assignedValue;
    AppendSubcategoriesDropdownWithFixedCategoryValues(drpCatid,assignedValue);

}
function AppendSubcategoriesDropdownWithFixedCategoryValues(drpCatId,assignedValue){
var categor=drpCatId;
console.log(categor);

var id = categor;
   id = id.replace(/drpCat/, '',$('#'+categor +' :selected').text(),'abcd');
    console.log(id);
    id=parseInt(id,10);
 let getObj=Categories.filter((res)=>res.Title==$('#'+categor +' :selected').text());
  console.log(getObj[0].MandatorySubcategory,'hello',Categories);
  if((getObj[0].MandatorySubcategory==true)){

$(".starcat"+id).css('display', 'inline');


}
else if((getObj[0].MandatorySubcategory==null && getObj[0].MandatorySubcategory==false)){
flag1=false;
$(".starcat"+id).css('display', 'none');
}


$('#drpSubCat'+id).empty();
var cat="<option>--select--</option>";
for(var i=0;i<SubCategories.length;i++){
var valuedrp=assignedValue;
	if(valuedrp ==SubCategories[i].Category.Id){
	cat+='<option value='+SubCategories[i].Id+'>'+SubCategories[i].Title+'</option>';
	}
}
 $('#drpSubCat'+id).append(cat);
 //fixed category Id 
 

}

