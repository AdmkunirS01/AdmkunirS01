function validateHours(){

var valid = true;
 var sum =0;
        for(var i = 0 ; i< valuesId.length; i++){
        sum+=parseInt($('#hours'+valuesId[i]).val(),10);

}
if(sum>_totalEffortLimit){
valid=false;
}
return valid;

}
function validateValues(){
var valid = true;
        for(var i = 0 ; i< valuesId.length; i++){
             if($('#drpCat'+valuesId[i]).val() == '--select--'){
             var element= document.getElementById('drpCat'+valuesId[i]);
             element.classList.add('hasError');
                valid = false;
                            }
            else {
            var element= document.getElementById('drpCat'+valuesId[i]);
                element.classList.remove("hasError");
                }
        	/*if($('#drpSubCat'+valuesId[i]).val() == '--select--'){
             var element= document.getElementById('drpSubCat'+valuesId[i]);
             element.classList.add('hasError');
                valid = false;
                            }
            else{
            var element= document.getElementById('drpSubCat'+valuesId[i]);
                element.classList.remove("hasError");
                }
                */
                 /*if($('#drpSubCat'+valuesId[i]).val() == '--select--' && $('#starcat'+valuesId[i]).is(":visible")){
                 flagCheckvalid=true;
                 }
                 else{
                 flagCheckvalid=false;
                 }*/

              //  if(flag1==true || flagCheckvalid==true){
               console.log('value'+ $('#drpSubCat'+valuesId[i]).val() );
                              console.log('visible'+$('.starcat'+valuesId[i]).is(":visible"));
             if(($('#drpSubCat'+valuesId[i]).val() == '--select--') &&( $('.starcat'+valuesId[i]).is(":visible"))){
             var element= document.getElementById('drpSubCat'+valuesId[i]);
             element.classList.add('hasError');
             console.log('if loop');
                valid = false;
                flag2=true;
                            }
            else {
            var element= document.getElementById('drpSubCat'+valuesId[i]);
                element.classList.remove("hasError");
                console.log('else loop');
                }
               // }
               
              
            if($('#hours'+valuesId[i]).val() == ''){
             var element= document.getElementById('hours'+valuesId[i]);
             element.classList.add('hasError');
                valid = false;
                            }
            else{
            var element= document.getElementById('hours'+valuesId[i]);
                element.classList.remove("hasError");
                }


               

        }
    return valid;
}

function selectCheckboxes(){

var flag=document.getElementById('selectall').checked;
if(document.getElementById('selectall').checked){
$("#effortBody").find('input[name="timesheet"]').each(function(){
$(this).prop('checked',true);

});

}
else{
$("#effortBody").find('input[name="timesheet"]').each(function(){
$(this).prop('checked',false);
});

}
}

function deleteRow(){
if(confirm("Do you want to delete selected row(s)?")){
$("#effortBody").find('input[name="timesheet"]').each(function(){
                if($(this).is(":checked")){
                	console.log(this.id);
                	var id=this.id;
                	id =(id)? id.replace(/timesheet/, ''):"0";
                	console.log(id);
                	var index = valuesId.indexOf(parseInt(id,10));
                	console.log(valuesId);
					if (index > -1) {
					  valuesId.splice(index, 1); // 2nd parameter means remove one item only
					}
					console.log(valuesId);
					if(parseInt($('#Item'+id).val(),10)>0){
					deleteValuesFromList(parseInt($('#Item'+id).val(),10));
					}
                	console.log($('#Item'+id).val());
                	

                    $(this).parents("tr").remove();
                }
            });
            }
            // new row adding if no rows ae there
var tbody = $("#effortBody");
     if(tbody.children().length == 0){
     AddRow();
    // alert("empty");
     }
     //end
     $("#selectall").prop('checked',false);

            ShowTotalHours()
}   

function deleteValuesFromList(param){
var listName="EffortTrackingList1";
 var itemType = GetItemTypeForListName("EffortTrackingList1");

$.ajax({
         url: _spPageContextInfo.siteAbsoluteUrl + "/_api/web/lists/getbytitle('" + listName + "')/getItemById("+param+")",
         type: "DELETE",
        
      	 headers: {
    	"accept":"application/json;odata=verbose",
		"content-type": "application/json;odata=verbose",
		"X-RequestDigest":$("#__REQUESTDIGEST").val(),
		"IF-MATCH":"*"      
			  },
        success: function (data) {
            console.log(data);
        },
        error: function (data) {
            console.log(data);
            CreateErrorLog(listName,data); 

        }
    });


}                

function setDefaultDate() {
    var date = new Date();
    var txtDate = document.getElementById('fromdate');
    var curr_date = date.getDate();
    var curr_month = date.getMonth() + 1;
    var curr_year = date.getFullYear();
    if (txtDate != undefined && txtDate != null) {
        txtDate.value = curr_month + "/" + curr_date + "/" + curr_year;
    }
}
function GetCategories(){
		 
    $.ajax({  
        url:  _spPageContextInfo.webAbsoluteUrl + "/_api/web/lists/getbytitle('Categories')/items?$select=Title,MandatorySubcategory,Id&$filter=Active eq 1&$orderBy=SortOrder"+"",  
        headers: {  
            "Access-Control-Allow-Origin":"*",
            "Accept": "application/json"  
        },  
        async: false,  
        success: function (data) {  
            console.log(data);
             Categories=data.value;
            console.log("Categories:",Categories);

                        
		},  
        eror: function (data) {  
            alert("An error occurred. Please try again."); 
            CreateErrorLog("Categories",data); 
 
        }  
    });  
	
}

function GetSubCategories(){
		 
    $.ajax({  
        url:  _spPageContextInfo.webAbsoluteUrl + "/_api/web/lists/getbytitle('Subcategories')/items?$select=Title,Id,MandetorySubcategory,Category/Title,Category/Id&$filter=Active eq 1&$orderBy=SortOrder&$expand=Category"+"",  
        headers: {  
            "Access-Control-Allow-Origin":"*",
            "Accept": "application/json"  
        },  
        async: false,  
        success: function (data) {  
            console.log(data);
           SubCategories=data.value;
            console.log(SubCategories,"==>");

                        
		},  
        eror: function (data) {  
            alert("An error occurred. Please try again.");  
            CreateErrorLog('Subcategories',data);
        }  
    });  
	
}
function GetSubNCategories(){
		 
    $.ajax({  
        url:  _spPageContextInfo.webAbsoluteUrl + "/_api/web/lists/getbytitle('SubNCategory')/items?$select=Title,Id,SubCategory/Title,SubCategory/Id&$filter=Active eq 1&$orderBy=SortOrder&$expand=SubCategory"+"",  
        headers: {  
            "Access-Control-Allow-Origin":"*",
            "Accept": "application/json"  
        },  
        async: false,  
        success: function (data) {  
            console.log(data);
             SubNCategories=data.value;
            console.log(SubNCategories);

                        
		},  
        eror: function (data) {  
            alert("An error occurred. Please try again."); 
            CreateErrorLog('SubNCategory',data); 
        }  
    });  
	
}

function checkFormattedHours(param){
var check=true;
for(var i=0;i<param.length;i++){
if(parseInt($('#hours'+param[i]).val(),10)%5 != 0){
var element= document.getElementById('hours'+param[i]);
             element.classList.add('hasError');

check =false;
}
else{
var element= document.getElementById('hours'+param[i]);
             element.classList.remove('hasError');

}
}

return check;
}
 function SubmitEffortDetails(param) {
     $(".se-pre-con").css("display", "block");  
   
 if(validateValues() ==false || validateHours()== false){
 if(validateValues() ==false){
 if(flag2==false){
 	alert("Category and Total % are mandatory.");
 	$(".se-pre-con").css("display", "none");
 	}
