public loadSearchedLocalPolicies(keyword, siteUrl) {
  // ... (existing code)

  return new Promise<any>((resolve, reject) => {
    // ... (existing code)

    // Assuming that SPWebUrl is an array
    resolve({ searchedIds, SPWebUrl }); // Return both arrays
  });
}


// Import the function from where it's defined
import { loadSearchedLocalPolicies } from 'path-to-your-file';

// Inside your component
// ...

// Assuming you're calling this function within a method or lifecycle hook
someMethod() {
  loadSearchedLocalPolicies(keyword, siteUrl)
    .then(data => {
      const { searchedIds, SPWebUrl } = data;
      // Now you can use searchedIds and SPWebUrl in your component
      // ...
    })
    .catch(error => {
      // Handle errors if any
    });
}



public loadSearchedLocalPolicies(keyword,siteUrl)

    {

      var _this = this;

      var searchedIds = [];

      var docSearchList=[];

      var SPWebUrl=[];

 

      return new Promise<any>((resolve, reject) =>

      {

       

        sp.search(<SearchQuery>{

        Querytext: keyword,

        RowLimit:_this.count,

        StartRow:_this.setRow,

        TrimDuplicates: false,

        SelectProperties : ["ListItemId","DefaultEncodingURL","SPWebUrl"],

        SourceId:'83817f38-03c1-425d-8b99-98cbcbce7cf7'

        }).

        then((p: SearchResults) => {

            console.log(p,'abcd')

           

            for(var j=0 ; j <p.PrimarySearchResults.length ; j++)

            {

                var ids = p.PrimarySearchResults[j]["ListItemId"];

               

                console.log(SPWebUrl,'SPWebUrl')

                if(ids != "")

                SPWebUrl[ids] =p.PrimarySearchResults[j]["SPWebUrl"]

                    searchedIds[ids] = p.PrimarySearchResults[j]["DefaultEncodingURL"];

               

                _this.setRow += 1;

               

            }

            docSearchList=[...searchedIds,...SPWebUrl]

            console.log(docSearchList,"9999")

           

            if(p.PrimarySearchResults.length == _this.count)

            {                  

                var result = this.loadSearchedLocalPolicies(keyword,siteUrl);

                result.then(function(data){

                    searchedIds =  _this.extend(data, searchedIds);

                    resolve(searchedIds);

                   

                   

                });

            }

            else

            {

                resolve(searchedIds);

            }

        });

     });

     

    }  

============

public loadSearchedLocalPolicies(keyword,siteUrl)

    {

      var _this = this;

      var searchedIds = {};

      return new Promise<any>((resolve, reject) =>

      {

       

        sp.search({Querytext : keyword+" path:"+siteUrl+"/Policy%20Documents",

        RowLimit:_this.count,

        StartRow:_this.setRow,

        TrimDuplicates: false,

        SelectProperties : ["ListItemID","DefaultEncodingURL"],

        }).

        then((p: SearchResults) => {

           

            for(var j=0 ; j <p.PrimarySearchResults.length ; j++)

            {

                var ids = p.PrimarySearchResults[j]["ListItemID"];

                if(ids != "")

                    searchedIds[ids] = p.PrimarySearchResults[j]["DefaultEncodingURL"];

                _this.setRow += 1;

                console.log(searchedIds,"9999")

            }

           

            if(p.PrimarySearchResults.length == _this.count)

            {                  

                var result = this.loadSearchedLocalPolicies(keyword,siteUrl);

                result.then(function(data){

                    searchedIds =  _this.extend(data, searchedIds);

                    resolve(searchedIds);

                   

                   

                });

            }

            else

            {

                resolve(searchedIds);

            }

        });

     });

     

    }  

 public searchLocalList(keyword,siteUrl)

    {

      var _this = this;

      var searchedIds = [];

      return new Promise<any>((resolve, reject) =>

      {

       

        sp.search({Querytext : keyword+" path:"+siteUrl+"/Lists/Policies/",

        RowLimit:_this.count,

        StartRow:_this.setRow,

        TrimDuplicates: false,

        SelectProperties : ["ListItemID"],

        }).

        then((p: SearchResults) => {

            for(var j=0 ; j <p.PrimarySearchResults.length ; j++)

            {

                var ids = p.PrimarySearchResults[j]["ListItemID"];

                if(ids != "")

                    searchedIds.push(parseInt(ids));

                _this.setRow += 1;

            }

            if(p.PrimarySearchResults.length == _this.count)

            {                  

                var result = this.searchLocalList(keyword,siteUrl);

                result.then(function(data){

                    searchedIds = searchedIds.concat(data);

